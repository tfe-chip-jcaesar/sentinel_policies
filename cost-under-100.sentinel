import "tfrun"
import "decimal"

print ( "Received value is: ", tfrun.cost_estimate.delta_monthly_cost )
raw_monthly_cost = tfrun.cost_estimate.delta_monthly_cost else "0"
print ( "Received value converted to: ", raw_monthly_cost )
delta_monthly_cost = decimal.new(raw_monthly_cost)

main = rule {
	delta_monthly_cost.less_than(100)
}
